<Window x:Class="Tortuga.Drydock.Views.DataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tortuga.Drydock.Views"
        mc:Ignorable="d"
        KeyUp="Window_KeyUp"
        
        Title="{Binding WindowTitle}" Height="600" Width="800">
    <DataGrid ItemsSource="{Binding Data.DefaultView}" IsReadOnly="True" AutoGenerateColumns="True" AutoGeneratingColumn="OnAutoGeneratingColumn">

    </DataGrid>
</Window>
